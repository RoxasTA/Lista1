#include <stdio.h>

int main(void){
    
    int a,b,c,d,calculo;
    
  
    
    printf("Valor de A:\n");
    scanf("%d",&a);
    printf("Valor de B:\n");
    scanf("%d",&b);
    printf("Valor de C:\n");
    scanf("%d",&c);
    printf("Valor de D:\n");
    scanf("%d",&d);
    
    calculo = (a * b - c * d);
    
    printf("DIFERENCA = %d * %d - %d * %d\n",a,b,c,d);
    printf("DIFERENCA = %d\n",calc);
    
    return 0;
}
