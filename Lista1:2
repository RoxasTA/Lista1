#include <stdio.h>
#include <math.h>

int main(void){
    
    float x1, y1, x2, y2;
    float distancia;
    
    printf("Digite as coordenadas de x e y do ponto 1: ");
    scanf("%f %f", &x1, &y1);
    
    printf("Digite as coordenadas de x e y do ponto 2: ");
    scanf("%f %f", &x2, &y2);
    
    distancia = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
    
    printf("Distancia entre os pontos Ã© (%.4f, %.4f) e (%.4f, %.4f): %.4f\n", x1, y1, x2, y2, distancia);
    
    return 0;
}
